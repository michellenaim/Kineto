<!DOCTYPE html>
<html>
  <head>
    <title>Kineto</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <link rel="stylesheet" href="" type="text/css">
    <script src="https://kit.fontawesome.com/7bdd1d32e3.js" crossorigin="anonymous"></script>
    <script type="text/javascript">
        window.kinetoLogo = "<%= image_url('Kineto_logo.png') %>";
     </script>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= favicon_link_tag asset_path('favicon.ico') %>
    <% if current_user %>
    <script>
      window.currentUser = {
        "id": <%= current_user.id %>, 
        "email": "<%= current_user.email %>",
      }
    </script>
    <% end%>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
